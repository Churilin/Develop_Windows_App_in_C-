<Window x:Class="itmo.CSDesctopApp.WpfRoutedEventTwo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:itmo.CSDesctopApp.WpfRoutedEventTwo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtBox" HorizontalAlignment="Center" Margin="0,137,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Height="37"/>
        <StackPanel ButtonBase.Click="StackPanel_Click" HorizontalAlignment="Left" Margin="350,40,0,0" VerticalAlignment="Top" Width="100">
            <Button x:Name="butAdd" Content="x+x"/>
            <Button x:Name="butMult" Content="x*x"/>
            <Button x:Name="butSqrt" Content="sqrt"/>
        </StackPanel>
        

    </Grid>
</Window>
